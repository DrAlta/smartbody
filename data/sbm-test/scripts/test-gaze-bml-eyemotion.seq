#	test-gaze-eyemotion.seq
#	Contributed by Brent Lance of USC

0	test bml <gaze target="camera"/>
0	echo test eyeball motion
0	test bml speech test eyeball motion

5	echo up 15 degrees
5	test bml speech up 15 degrees
5	test bml <gaze target="camera" direction="UP" angle="15" sbm:joint-range="eyes" sbm:joint-speed="180 180 180"/>

10	echo up right 25 
10	test bml speech up right 25 
10	test bml <gaze target="camera" direction="UPRIGHT" angle="25" sbm:joint-range="eyes" sbm:joint-speed="180 180 180"/>

15	echo right 45 
15	test bml speech right 45 
15	test bml <gaze target="camera" direction="RIGHT" angle="45" sbm:joint-range="eyes" sbm:joint-speed="180 180 180"/>

20	echo down right 30 
20	test bml speech down right 30 
20	test bml <gaze target="camera" direction="DOWNRIGHT" angle="30" sbm:joint-range="eyes" sbm:joint-speed="180 180 180"/>

25	echo down 20 
25	test bml speech down 20 
25	test bml <gaze target="camera" direction="DOWN" angle="20" sbm:joint-range="eyes" sbm:joint-speed="180 180 180"/>

30	echo down left 30
30	test bml speech down left 30
30	test bml <gaze target="camera" direction="DOWNLEFT" angle="30" sbm:joint-range="eyes" sbm:joint-speed="180 180 180"/>

35	echo left 45
35	test bml speech left 45
35	test bml <gaze target="camera" direction="LEFT" angle="45" sbm:joint-range="eyes" sbm:joint-speed="180 180 180"/>

40	echo up left 25
40	test bml speech up left 25
40	test bml <gaze target="camera" direction="UPLEFT" angle="25" sbm:joint-range="eyes" sbm:joint-speed="180 180 180"/>

45	echo reset
45	test bml <gaze target="camera" sbm:joint-speed="180 180 180"/>
